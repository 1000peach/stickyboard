{
    "name": "StickyBoard",
    "version": "0.1.0",
    "private": true,
    "author": "soaple",
    "scripts": {
        "start": "nodemon app.js",
        "build-lib-dev": "webpack --config webpack.lib.dev.js",
        "build-lib-prod": "webpack --config webpack.lib.prod.js",
        "watch-app-dev": "webpack-dev-server --progress --inline --hot --config=webpack.app.dev.js",
        "build-app-dev": "webpack --config webpack.app.dev.js",
        "build-app-prod": "webpack --config webpack.app.prod.js",
        "clean": "rimraf dist",
        "watch": "npm run clean && npm run build-lib-dev && npm run watch-app-dev",
        "dev": "NODE_ENV=development nodemon app.js",
        "build": "npm run clean && npm run build-lib-prod && npm run build-app-prod",
        "production": "NODE_ENV=production nodemon app.js"
    },
